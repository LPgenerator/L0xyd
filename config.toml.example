# API listen port
api-address = "0.0.0.0:8081"

# LB listen port
lb-address = "127.0.0.1:8080"

# Log all into file
lb-log-file = "/var/log/lpg-lb.log"

# Default instances under Load Balancer
[servers]
  [servers.web-1]
  url = "http://127.0.0.1:8081"
  weight = 0

  [servers.web-2]
  url = "http://127.0.0.1:8082"
  weight = 0
